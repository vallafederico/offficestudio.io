---
const { title, description, keyworks } = Astro.props;
---

<!-- page -->
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width" />

<!-- basic -->
<meta name="description" content={description} />
<meta name="keywords" content={keyworks || "Contemporary Design Studio"} />

<meta property="og:url" content="https://offficestud.io/" />
<meta property="og:type" content="website" />
<meta property="og:title" content="For Future Furnishing" />
<meta property="og:description" content="Home Page" />
<meta
  property="og:image"
  content="https://offficestud.io/util/offfice-og.png"
/>

<meta name="twitter:card" content="summary_large_image" />
<meta property="twitter:domain" content="offficestud.io" />
<meta property="twitter:url" content="https://offficestud.io/" />
<meta name="twitter:title" content="For Future Furnishing" />
<meta name="twitter:description" content="Home Page" />
<meta
  name="twitter:image"
  content="https://offficestud.io/util/offfice-og.png"
/>
